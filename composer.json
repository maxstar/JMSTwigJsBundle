{
    "name": "jms/twig-js-bundle",
    "type": "symfony-bundle",
    "description": "Integrates twig.js into your Symfony2 application",
    "keywords": [
        "templating"
    ],
    "homepage": "http://jmsyst.com/bundles/JMSTwigJsBundle",
    "license": "Apache2",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "github",
            "no-api": true,
            "url": "https://github.com/maxstar/twig.js.git"
        }
    ],
    "require": {
        "php": ">=7.0",
        "jms/twig-js": "dev-twig-2",
        "symfony/framework-bundle": "^2.7|^3.0|^4.0",
        "symfony/translation": "^2.7|^3.0|^4.0",
        "symfony/twig-bridge": "^2.7|^3.0|^4.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.3",
        "symfony/console": "^2.7|^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "JMS\\TwigJsBundle\\": ""
        }
    }
}
